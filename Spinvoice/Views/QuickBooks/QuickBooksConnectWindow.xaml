<Window x:Class="Spinvoice.Views.QuickBooks.QuickBooksConnectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Spinvoice.Views"
        xmlns:viewModels="clr-namespace:Spinvoice.QuickBooks.ViewModels;assembly=Spinvoice.QuickBooks"
        Title="Connect to QuickBooks"
        d:DataContext="{d:DesignInstance viewModels:QuickBooksConnectViewModel}"
        mc:Ignorable="d">
    <Grid Margin="10">
        <WebBrowser views:WebBrowserUtility.BindableSource="{Binding Url, Mode=TwoWay}" Navigated="WebBrowser_OnNavigated" />
    </Grid>
</Window>
