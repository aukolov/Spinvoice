             DispatcherUnhandledException="App_OnDispatcherUnhandledException"
             StartupUri="pack://application:,,,/Spinvoice;component/Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Spinvoice;component/Views/SharedResources.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
